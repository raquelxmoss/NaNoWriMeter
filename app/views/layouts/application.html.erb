<!DOCTYPE html>
<html>
<head>
  <title>NaNoWriMeter</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= current_user ? "You are signed in as #{current_user.github_username}" : link_to "Sign in with Github", user_omniauth_authorize_path(:github) %>
  <%= yield %>
</body>
</html>
